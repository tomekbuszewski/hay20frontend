{
  "name": "ts-react",
  "version": "0.0.1",
  "description": "React starter with TypeScript, ESLint and Prettier",
  "main": "public/index.ts",
  "scripts": {
    "~~~~~~ DEV COMMANDS ~~~~~~": "",
    "commit": "git-cz --disable-emoji",
    "clear-public": "rm -rf ./public/assets",
    "hmr": "npm run build:dev && NODE_ENV=development webpack-dev-server --hot",
    "hmr:mocks": "npm run build:dev && cross-env NODE_ENV=development MOCKS=true webpack-dev-server --hot",
    "plop": "plop",
    "postplop": "pretty-quick",
    "serve:prod": "npm run build:prod; http-server ./public -p 9998 --cors",
    "storybook": "start-storybook -p 9001 -c ./@config/storybook",
    "test": "jest --coverage",
    "~~~~~~ BUILD COMMANDS ~~~~~~": "",
    "assets": "cp ./assets/* ./public/assets",
    "manifest": "cp ./manifest.json ./public",
    "sw": "cp ./public/assets/service-worker.js ./public/; cp ./public/assets/workbox-*.js ./public/",
    "after-build": "npm run assets && npm run manifest && npm run sw",
    "build:dev": "NODE_ENV=development webpack && npm run after-build",
    "build:prod": "npm run clear-public; NODE_ENV=production webpack --mode=production && npm run after-build",
    "~~~~~~ DEPLOY COMMANDS ~~~~~~": "",
    "deploy:dev": "surge ./public https://hay-dev.surge.sh",
    "deploy:prod": "surge ./public https://hay.surge.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tomekbuszewski/ts-react-starter.git"
  },
  "keywords": [
    "react",
    "typescript",
    "starter"
  ],
  "author": "Tomek Buszewski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomekbuszewski/ts-react-starter/issues"
  },
  "homepage": "https://github.com/tomekbuszewski/ts-react-starter#readme",
  "dependencies": {
    "@loadable/component": "^5.13.2",
    "babel-plugin-remove-test-ids": "^1.1.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "http-server": "^0.12.3",
    "ky": "^0.23.0",
    "ky-universal": "^0.8.2",
    "polished": "^3.6.6",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.12.21",
    "react-query": "^2.23.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "serialize-javascript": "^5.0.1",
    "styled-components": "^5.2.0",
    "styled-reboot": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-backgrounds": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/cli": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@sucrase/webpack-loader": "^2.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.14",
    "@types/loadable__component": "^5.13.0",
    "@types/node": "^14.10.2",
    "@types/react": "^16.9.49",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-query": "^1.1.2",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-transition-group": "^4.4.0",
    "@types/serialize-javascript": "^4.0.0",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-loader": "^8.1.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-react": "^7.20.6",
    "git-cz": "^4.7.1",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "inquirer": "^7.3.3",
    "jest": "^26.4.2",
    "msw": "^0.21.2",
    "plop": "^2.7.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "stylelint": "^13.7.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "sucrase": "^3.15.0",
    "terser-webpack-plugin": "^4.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
